{
  "name": "${#projectName#}",
  "version": "1.0.0",
  "description": "${#projectDescription=none#}",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "fisx server start",
    "prod": "fisx server start --release",
    "build": "rm -rf output && fisx release -c",
    "release": "rm -rf output && fisx release prod -c"
  },
  "author": "",
  "repository": "",
  "license": "BSD",
  "${#manifestKey#}": {
    "requireConfig": {
      "baseUrl": "src",
      "paths": {
        "babelHelpers": "babelHelpers"
      }
    },
    "dependencies": {
      "vue": "^1.0.0"
    },
    "ignoreDependencies": [
      "envify"
    ],
    "lock": {
      "vue": {
        "endpoint": "npm"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "fis3-preprocessor-replacer": "^0.1.0",
    "fisx-preprocessor-amd": "^0.1.1",
    "fisx-preprocessor-babel": "^0.1.0",
    "fisx-parser-vue": "^0.1.0",
    "fisx-vue1-loader": "^0.1.0",
    "html-minifier": "^3.2.3",
    "stylus": "^0.54.5",
    "rider": "^2.0.0",
    "autoprefixer-stylus": "^0.11.0",
    "qrequire": "^0.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "external-helpers"
    ]
  }
}
